{
	"name": "ui",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev:content": "velite --watch",
		"build:content": "velite --clean",
		"build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./src/scripts/build-registry.mts && biome format --write ./src/__registry__/**.* && biome format --write ./src/registry/index.json",
		"dev:next": "next dev",
		"build:next": "next build",
		"dev": "run-p dev:*",
		"build": "run-s build:*",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"format": "biome check --write . && biome format --write .",
		"clean": "find . -name node_modules -o -name .next -o -name .turbo -o -name dist -type d -prune | xargs rm -rf",
		"clean:lock": "rm -f pnpm-lock.yaml",
		"clean:resync": "pnpm clean:lock && pnpm install",
		"bump:latest": "pnpm up -i -latest"
	},
	"dependencies": {
		"@emotion/is-prop-valid": "^1.2.2",
		"@hookform/resolvers": "^3.9.0",
		"@internationalized/date": "^3.5.4",
		"@radix-ui/react-accordion": "^1.2.0",
		"@radix-ui/react-scroll-area": "^1.1.0",
		"@react-aria/toast": "3.0.0-beta.12",
		"@react-stately/toast": "3.0.0-beta.4",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"embla-carousel-react": "^8.1.6",
		"framer-motion": "^11.3.0",
		"lodash.template": "^4.5.0",
		"lucide-react": "^0.407.0",
		"next": "14.2.5",
		"next-themes": "^0.3.0",
		"react": "^18",
		"react-aria": "^3.33.1",
		"react-aria-components": "^1.2.1",
		"react-dom": "^18",
		"react-hook-form": "^7.52.1",
		"react-query": "^3.39.3",
		"react-stately": "^3.31.1",
		"react-wrap-balancer": "^1.1.1",
		"shiki": "^1.10.3",
		"tailwind-merge": "^2.4.0",
		"tailwindcss-animate": "^1.0.7",
		"tailwindcss-react-aria-components": "^1.1.3",
		"ts-morph": "^23.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@shikijs/compat": "^1.10.3",
		"@types/eslint": "^8.56.2",
		"@types/node": "^20.14.10",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/unist": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^7.16.0",
		"@typescript-eslint/parser": "^7.16.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"eslint-plugin-only-warn": "^1.1.0",
		"mdast-util-toc": "^7.1.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.39",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-pretty-code": "^0.13.2",
		"rehype-slug": "^6.0.0",
		"remark": "^15.0.1",
		"remark-code-import": "^1.2.0",
		"remark-gfm": "^4.0.0",
		"rimraf": "^6.0.1",
		"tailwindcss": "^3.4.1",
